	</module>

	<!-- Plans -->
	<module name="plans">
		<param name="inputPlansFile" value="F:\PSL\Documentte\Nistor\Educatie\Doctorat\Articole\UPB Bulletin\optimized_population.xml"/>
	</module>

	<!-- Controler -->
	<module name="controller">
